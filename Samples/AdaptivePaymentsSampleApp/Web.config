    </httpHandlers> 
	</system.web>

	<system.webServer>
    <defaultDocument enabled="true">
      <files>
        <add value="~/Default.aspx"/>
      </files>
    </defaultDocument>
  </system.webServer>
  
  <appSettings>
    <add key="PAYPAL_REDIRECT_URL" value="https://www.sandbox.paypal.com/webscr&amp;cmd="/>
  </appSettings>
